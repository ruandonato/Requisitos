<div class="page-header">
  <%= link_to kanbans_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_kanban_path(@kanban), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show kanban</h1>
</div>

<dl class="dl-horizontal">
  <dt>Project:</dt>
  <dd><%= @kanban.project %></dd>

  <dt>Stories:</dt>
  <dd><%= @kanban.stories %></dd>

</dl>
